    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <!-- 右上角popupwindow -->
    <style name="AnimationMainTitleMore">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="ac_search_list_view_divider">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/seal_search_item_devi_height</item>
        <item name="android:background">@color/seal_search_devi_color</item>
        <item name="android:layout_marginLeft">@dimen/seal_search_devi_color_margin_left</item>
        <item name="android:layout_marginRight">@dimen/seal_search_devi_color_margin_right</item>
    </style>

    <style name="SwitchButtonStyle">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="ac_search_list_view_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/seal_search_item_title_height</item>
        <item name="android:paddingLeft">@dimen/seal_search_item_title_padding_left</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/seal_search_item_title_text_size</item>
        <item name="android:textColor">@color/seal_search_description_text_color</item>
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="seal_horizontal_light_thin_divider">
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/color_list_item_hover</item>
    </style>
    <style name="seal_vertical_light_thin_divider">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0.5dp</item>
        <item name="android:background">@color/color_divider_line</item>
    </style>
    <style name="rce_horizontal_light_thin_divider">
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/color_list_item_hover</item>
    </style>

    <style name="seal_profile_user_detail_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginLeft">14dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginRight">14dp</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="input_editview" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/seal_search_line_color</item>
        <item name="colorControlActivated">@color/text_blue</item>
    </style>

    <style name="TranslucentNoTitleActivity" parent="@style/Base.Theme.AppCompat.Dialog">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>

    <style name="DeepBlackTranslucentNoTitleActivity" parent="@style/Base.Theme.AppCompat.Dialog">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@color/deep_black_transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>

    <!--GroupUserInfo-->
    <style name="GroupUserInfoEditText">
        <item name="android:background">@null</item>
        <item name="android:gravity">right|center</item>
        <item name="android:textColor">@color/color_black_text</item>
        <item name="android:textColorHint">@color/seal_login_text_button_color</item>
        <item name="android:textSize">15sp</item>
    </style>

    <!--EditUserDes-->
    <style name="EditUserDesEditText">
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/color_black_text</item>
        <item name="android:textColorHint">@color/seal_login_text_button_color</item>
        <item name="android:textSize">15sp</item>
    </style>











    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="SplashTheme" parent="AppTheme.NoActionBar">
        <item name="android:background">@drawable/seal_ic_splash</item>
    </style>
    <style name="AppTheme.NoActionBar" >
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="lineStyle">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="background">@color/line</item>
    </style>

    <style name="tv16BlackStyle">
        <item name="android:textColor">@color/color_black_text</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="tv14BlackBStyle">
        <item name="android:textColor">@color/blackB</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="tv13GrayBStyle">
        <item name="android:textColor">@color/grayB</item>
        <item name="android:textSize">@dimen/sp_13</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="tv16WhiteStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="tv12RedStyle">
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:textColor">@color/color_red_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:layout_marginLeft">@dimen/dp_12</item>
    </style>

    <style name="etPwdStyle">
        <item name="android:maxLength">20</item>
        <item name="android:textColor">@color/color_black_text</item>
        <item name="android:inputType">textPassword</item>
        <item name="android:textColorHint">#8F929A</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:background">@drawable/gray_4dp_bg</item>
        <item name="android:paddingLeft">@dimen/dp_10</item>
        <item name="android:layout_marginLeft">@dimen/dp_12</item>
        <item name="android:layout_marginRight">@dimen/dp_12</item>
        <item name="android:layout_marginTop">@dimen/dp_16</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_45</item>
    </style>

    <style name="DatePickerBottomDialog" parent="android:Theme.Light">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="DatePickerDialogAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/com_ycuwq_datepicker_dialog_in</item>
        <item name="android:windowExitAnimation">@anim/com_ycuwq_datepicker_dialog_out</item>
    </style>

    <style name="searchEtStyle">
        <item name="android:paddingLeft">@dimen/dp_15</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_30</item>
        <item name="android:layout_marginTop">@dimen/dp_5</item>
        <item name="android:layout_marginBottom">@dimen/dp_5</item>
        <item name="android:layout_marginLeft">@dimen/dp_12</item>
        <item name="android:layout_marginRight">@dimen/dp_12</item>
        <item name="android:drawableLeft">@drawable/img_search</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textColor">@color/blackA</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/seal_search_title_bar_et_text_size</item>
        <item name="android:imeOptions">actionSearch</item>
        <item name="android:inputType">textNoSuggestions</item>
        <item name="android:imeActionLabel"></item>
        <item name="android:background">@drawable/search_bg</item>
    </style>


</resources>
